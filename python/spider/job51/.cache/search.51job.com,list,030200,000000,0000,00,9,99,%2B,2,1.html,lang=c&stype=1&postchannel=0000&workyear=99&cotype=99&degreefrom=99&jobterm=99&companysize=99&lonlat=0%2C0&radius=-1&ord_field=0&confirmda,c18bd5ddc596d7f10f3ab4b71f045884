    <script>
        $(function(){//返回顶部
            var stop='';
            $(window).scroll(function(){
                $('#goTop').hide();
                stop=$(this).scrollTop();
                if(stop>0){
                    $('#goTop').show();
                }else{
                    $('#goTop').hide();
                }
            });
        })
    </script>
