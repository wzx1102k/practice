                <a class="newlist_sqzw selectall-btn" href="javascript:zlapply.searchjob.ajaxApplyBrig2(document.frmMain.vacancyid,'ssb');dyweTrackEvent('applyNow','joblist');submitLogBatch(4);">申请职位</a>
