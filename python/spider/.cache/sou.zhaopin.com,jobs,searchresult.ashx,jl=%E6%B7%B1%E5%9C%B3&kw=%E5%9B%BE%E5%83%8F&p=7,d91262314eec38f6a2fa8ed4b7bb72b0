    if (typeof (window['flagZhaopinAnalytics']) == 'undefined' || !window['flagZhaopinAnalytics']) {
        var _dyweq = _dyweq || [];
        _dyweq.push(['_setAccount', window['acc4zpAnalytics'] || 'log000001']);
