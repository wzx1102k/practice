        else _dyweq.push(['_trackPageview']);

        (function () {
            var dywe = document.createElement('script');
            dywe.type = 'text/javascript';
            dywe.async = true;
            dywe.src = 'http://img01.zhaopin.cn/2012/js/dywe.js';
